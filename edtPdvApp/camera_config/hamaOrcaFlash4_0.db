	field( INPA, "$(P)$(R)EdtCamRepDur          CPP MS" )
	field( INPB, "$(P)$(R)OrcaRollingShutterRamp CPP MS" )
	field( INPC, "$(P)$(R)AcquireTime_RBV        CPP MS" )
